package skeletoncodeforaa;

/**
 *
 * @author Vaio
 */
public class GenerationIndexOutOfBoundsException extends Exception {

    /**
     * Creates a new instance of <code>GenerationIndexOutOfBoundsException</code> without detail
     * message.
     */
    public ComboException() {
    }

    /**
     * Constructs an instance of <code>GenerationIndexOutOfBoundsException</code> with the specified
     * detail message.
     *
     * @param msg the detail message.
     */
    public ComboException(String msg) {
        super(msg);
    }
}
